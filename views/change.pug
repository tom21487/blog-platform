extends basicHTML

block content
  each post of posts
    div
      | #{post.titleEn} | #{post.titleCn} 
      | (
      a(href='/'+language+`/control/change/${type}/${post._id}/update`) update
      | ) (
      a(href='/'+language+`/control/change/${type}/${post._id}/delete`) delete
      | )
  a(href='/'+language+'/control/change' style="display: block") Back
